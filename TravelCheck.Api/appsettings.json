{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Azure.Cosmos": "Information"
    }
  },

  "AllowedHosts": "*",

  "CosmosDb": {
    "AccountEndpoint": "https://YOUR-ACCOUNT.documents.azure.com:443/",
    "AccountKey": "YOUR-PRIMARY-KEY",
    "DatabaseName": "TravelCheckDb",
    "ContainerName": "Trips"
  },

  "BlobStorage": {
    "ConnectionString": "DefaultEndpointsProtocol=https;AccountName=YOURSTORAGE;AccountKey=KEY;EndpointSuffix=core.windows.net",
    "ContainerName": "attachments"
  },

  "ServiceBus": {
    "ConnectionString": "Endpoint=sb://YOURNS.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=KEY",
    "QueueName": "trip-events"
  },

  "ApplicationInsights": {
    "ConnectionString": "InstrumentationKey=XXXX;IngestionEndpoint=https://westeurope-0.in.applicationinsights.azure.com/"
  }
}
